#################
#### .BASHRC ####
#################


#################LS DIRCOLORS
[[ $- != *i* ]] && return
eval "`dircolors -b ~/.dircolors`"
alias ls='ls --color=auto --file-type'

#################LS EJECUTABLES
function lsx() { 
find "$@" -maxdepth 1 -perm -ugo+x -type f 
}

#################PROMPT
PS1='\[\033[01;38;5;202m\] \W \$\[\e[0m\] '
